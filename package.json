{
  "name": "newshortcuturlwindows",
  "displayName": "NewShortcutUrlWindows",
  "description": "generate .url file for windows.",
  "repository": {
    "url": "https://github.com/ichir0roie/newshortcuturlwindows"
  },
  "author": {
    "name": "ichir0roie"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:newshortcuturlwindows.newshortcut"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "newshortcuturlwindows.newshortcut",
        "title": "New Shortcut"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "newshortcuturlwindows.newshortcut",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
